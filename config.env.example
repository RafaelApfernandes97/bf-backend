# Configurações do Servidor
NODE_ENV=development
PORT=3001

# Configurações do AWS S3
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
S3_BUCKET=your-s3-bucket-name
S3_BUCKET_PREFIX=balletemfoco

# Configurações do MongoDB
MONGODB_URI=mongodb://localhost:27017/fotos-ballet

# Configurações de Autenticação
JWT_SECRET=your-super-secret-jwt-key
GOOGLE_CLIENT_ID=your-google-client-id

# Configurações do Redis
REDIS_URL=redis://localhost:6379

# Configurações de Performance da Indexação
INDEXACAO_CONCURRENT=8          # Número máximo de fotos processadas simultaneamente (padrão: 8)
INDEXACAO_BATCH_SIZE=20         # Tamanho dos lotes de processamento (padrão: 20)
INDEXACAO_RETRY_ATTEMPTS=3      # Número de tentativas em caso de falha (padrão: 3)
INDEXACAO_RETRY_DELAY=1000      # Delay entre tentativas em ms (padrão: 1000) 